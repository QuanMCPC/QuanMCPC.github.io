    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "pwa-chrome",
        "url": "http://127.0.0.1:5500/offline.html",
        "webRoot": "${workspaceFolder}"
    },
        {
            "name": "Launch Console",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://127.0.0.1:5500/other_project/console.html",
            "webRoot": "${workspaceFolder}",
            "runtimeExecutable": "C:/Users/tuana/AppData/Local/CocCoc/Browser/Application/browser.exe"
        },

        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Home",
            "url": "http://127.0.0.1:5500/home.html",
            "webRoot": "${workspaceFolder}",
            "runtimeExecutable": "C:/Users/tuana/AppData/Local/CocCoc/Browser/Application/browser.exe"
        }
    ]
}